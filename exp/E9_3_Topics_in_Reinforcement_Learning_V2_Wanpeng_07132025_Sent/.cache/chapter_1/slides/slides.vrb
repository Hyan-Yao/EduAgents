import numpy as np

# Initialize Q-table
Q = np.zeros((state_space_size, action_space_size))

# Q-learning parameters
alpha = 0.1   # Learning rate
gamma = 0.99  # Discount factor

# Q-learning update
Q[state, action] += alpha * (reward + gamma * np.max(Q[next_state]) - Q[state, action])
