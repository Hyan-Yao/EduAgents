    \begin{block}{Key Formulas to Remember}
        \begin{equation}
            J = \sum_{t=0}^{N} \left( x_t^T Q x_t + u_t^T R u_t \right)
        \end{equation}
        where:
        \begin{itemize}
            \item \( x_t \) = state vector
            \item \( u_t \) = control input
            \item \( Q, R \) = weight matrices.
        \end{itemize}
    \end{block}
